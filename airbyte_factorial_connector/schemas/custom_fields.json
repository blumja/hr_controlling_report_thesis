{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": ["integer"]
        },
        "label": {
            "type": ["string"]
        },
        "position": {
            "type": ["null", "integer"]
        },
        "required": {
            "type": ["boolean"]
        },
        "field_type": {
            "type": ["string"]
        },
        "min_value": {
            "type": ["null", "integer"]
        },
        "max_value": {
            "type": ["null", "integer"]
        },
        "choice_options": {
            "type": ["array"],
            "items": {
                "type": ["null", "object"],
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "label": {
                        "type": "string"
                    },
                    "value": {
                        "type": "string"
                    },
                    "is_active": {
                        "type": "boolean"
                    }
                }
            }
        }
    }
}
